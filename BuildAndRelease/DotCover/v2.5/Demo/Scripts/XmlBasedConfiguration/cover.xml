<?xml version="1.0" encoding="utf-8"?>
<CoverageParams>
  <TargetExecutable>..\..\NUnit\nunit-console.exe</TargetExecutable>
  <TargetArguments>SampleApp.Util.Tests.dll</TargetArguments>
  <TargetWorkingDir>..\..\SampleApp\Bin</TargetWorkingDir>
  <Output>..\..\Snapshots\SampleApp.Util.Tests.dcvr</Output>
  
  <Filters>
    <IncludeFilters>
      <FilterEntry>
        <ModuleMask>*</ModuleMask>
        <ClassMask>*</ClassMask>
        <FunctionMask>*</FunctionMask>
      </FilterEntry>
    </IncludeFilters>
    <ExcludeFilters>
      <FilterEntry>
        <ModuleMask>*Tests*</ModuleMask>
        <ClassMask>*</ClassMask>
        <FunctionMask>*</FunctionMask>
      </FilterEntry>
    </ExcludeFilters>
  </Filters>
</CoverageParams>